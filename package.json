{
  "name": "expo-downloads",
  "version": "0.1.1",
  "description": "An Expo module for downloading files directly to the native Downloads folder on Android and iOS.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "pnpm run /^lint:/",
    "lint:expo": "expo-module lint",
    "lint:prettier": "prettier --check \"src/**/*.{ts,tsx}\"",
    "fix": "pnpm run /^fix:/",
    "fix:expo": "expo-module lint --fix",
    "fix:prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "xed example/ios",
    "open:android": "open -a \"Android Studio\" example/android",
    "release": "bumpp -c 'release: v%s' -t 'v%s'"
  },
  "keywords": [
    "react-native",
    "expo",
    "expo-downloads",
    "Downloads"
  ],
  "repository": "https://github.com/pocketsign/expo-downloads",
  "bugs": {
    "url": "https://github.com/pocketsign/expo-downloads/issues"
  },
  "author": "PocketSign Inc.",
  "license": "MIT",
  "homepage": "https://github.com/pocketsign/expo-downloads",
  "devDependencies": {
    "@types/react": "~19.0.14",
    "bumpp": "^10.2.3",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-universe": "^15.0.3",
    "eslint-plugin-prettier": "^5.5.4",
    "expo": "~53.0.20",
    "expo-module-scripts": "^4.1.10",
    "prettier": "^3.6.2",
    "react-native": "0.79.5"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "packageManager": "pnpm@10.14.0"
}
